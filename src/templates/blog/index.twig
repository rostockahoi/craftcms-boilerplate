{% extends "_layouts/_master" %}

{% set title = "News" %}

{% block content%}
    <h1>News</h1>
    <ul>
        {% for entry in craft.entries.section('news').limit(5).find() %}
            <li>
                <a href="{{ entry.url }}">{{ entry.title }}</a>
            </li>
        {% endfor %}
    </ul>
{% endblock %}
