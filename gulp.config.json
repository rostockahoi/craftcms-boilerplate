{
    "url": 				"craft.dev",
    "src": {
        "styles": 		"src/styles",
        "scripts": 		"src/scripts",
        "templates": 	"src/templates",
        "fonts": 		"src/fonts",
        "images": 		"src/img",
        "favicon": 		"src/favicon/favicon.png"
    },
    "dist": {
    	"styles": 		"public/assets/css",
    	"scripts": 		"public/assets/js",
    	"templates": 	"craft/templates",
    	"fonts": 		"public/assets/fonts",
    	"images": 		"public/assets/img",
    	"favicon": 		"public/favicon"
    },
    "styles": {
    	"compatibility": [
	        "last 2 versions",
	        "ie >= 9"
	    ],
	    "includePaths": []
    },
    "favicon": {
    	"templateFile": "_favicon.html",
    	"background": "#FFFFFF",
    	"theme_color": "#FFFFFF",
    	"path": "favicon/",
    	"display": "standalone",
    	"orientation": "portrait",
    	"start_url": "/?homescreen=1",
    	"pipeHTML": true,
    	"replace": true
    }
}
